      "mainAsset": "data.students.data.[].attributes.mainAsset.data.attributes",
      "people": "data.students.data.[].attributes.profilePicture.data.attributes"
    },
    "project": {
      "people": {
        "key": "attributes"
      }
    }
  },
  "planes": {
    "namePlane": {
      "height": 280,
      "maxHeight": 360,
      "marginTop": 120,
      "name": {
        "template": "${firstName} ${lastName}",
        "font": "700 80px 'Embedded VAGRounded'",
        "fillStyle": "rgb(68,200,245)",
        "marginBottom": 70
      },
      "tagLine": {
        "template": "Alumnus ${curriculum.data.attributes.name}",
        "font": "400 48px 'Embedded VAGRounded'",
        "fillStyle": "#000000"
      }
    },
    "bioPlane": {
      "backgroundColor": "#ffffff",
      "marginTop": 98,
      "gradientTop": {
        "height": 50,
        "y": 100,
        "stops": {
          "0": "rgba(255,255,255,1)",
          "1": "rgba(255,255,255,0)"
        }
      },
      "gradientBottom": {
        "height": 50,
        "stops": {
          "0": "rgba(255,255,255,0)",
          "1": "rgba(255,255,255,1)"
        }
      },
      "title": {
        "template": "BIO",
        "font": "700 53px 'Embedded VAGRounded'",
        "fillStyle": "rgb(68, 200, 245)",
        "marginBottom": 100
      },
      "paragraphs": {
        "template": "${attributes.bio}",
        "font": "400 53px 'Embedded OpenSans'",
        "fillStyle": "#000000",
        "lineHeight": 70
      }
    }
  },
